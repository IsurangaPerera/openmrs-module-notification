<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC
        "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="org.openmrs.module.notifications.entity.Event" table="event">

        <id name="id" type="int" column="id" unsaved-value="0">
            <generator class="native"/>
        </id>

        <property name="name" type="java.lang.String" column="name" length="30"/>
        <property name="description" type="java.lang.String" column="description" length="200"/>

    </class>
</hibernate-mapping>
